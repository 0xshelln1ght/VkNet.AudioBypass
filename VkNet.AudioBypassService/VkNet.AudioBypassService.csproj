<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net40;net45;net461</TargetFrameworks>
		<Version>1.5.1</Version>
		<Description>
			Расширение для VkNet для обхода ограничения к методам Audio и Messages ВКонтакте API.
		</Description>
		<PackageReleaseNotes>
			Совместимость с VkNet 1.45.0.
		</PackageReleaseNotes>
		<Authors>shelln1ght;itsokto</Authors>
		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/atckun/VkNetMusicBypass</RepositoryUrl>
		<PackageTags>vknet;vk;api;audio;bypass</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="JetBrains.Annotations" Version="2019.1.3" PrivateAssets="all" />
		<PackageReference Include="SonarAnalyzer.CSharp" Version="7.15.0.8572" PrivateAssets="all" />
		<PackageReference Include="VkNet" Version="1.45.0" />
		<PackageReference Include="protobuf-net" Version="2.4.0" />
	</ItemGroup>
</Project>